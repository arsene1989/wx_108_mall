@import '/style/layout.wxss';
.container {
  background-color: #F2f2f2;
}

.setting_container {
  margin-top: 25pt;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #FFFFFF;
  flex-direction: column;
}

.setting_item {
  align-items: center;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 35pt;
  background-color: #FFFFFF;
}

.setting_text {
  color: #000000;
  padding-left: 5pt;
  width: 30%;
}

.setting_line {
  width: 100%;
  height: 1pt;
  background-color: #F2f2f2;
}

.arrow {
  width: 20pt;
  height: 20pt;
  margin-left: 65%;
}

.download_container {
  display: flex;
  width: 100%;
  height: 50pt;
  align-items: center;
  justify-content: center;
  background-color: #FFFFFF;
  color: #CD9967;
}



