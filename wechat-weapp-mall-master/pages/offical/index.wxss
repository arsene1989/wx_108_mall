@import '/style/layout.wxss';
.container {
  display: flex;
  width: 100%;
  height: 100%;
}

.goods_item {
  width: 100%;
  height: 45pt;
  /* position: relative; */
  display: flex;
  align-items: center;
  border-bottom: 1px solid #dadada;
  align-content: row;
  background-color: #FFFFFF;
} 
.goods_image {
  width: 100rpx;
  height: 100rpx;
  margin-left: 10pt;
}
.price {
  margin-left: 5pt;
  width: 30%;
  text-align:right;
}